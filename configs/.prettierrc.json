{
    "semi": true,
    "overrides": [
        {
            "files": "*.js",
            "options": {
                "semi": true,
                "singleQuote": false,
                "trailingComma": "none"
            }
        },
        {
            "files": "*.json",
            "options": {
                "trailingComma": "none"
            }
        },
        {
            "files": ["*.html"],
            "options": {
                "tabWidth": 4
            }
        }
    ]
}

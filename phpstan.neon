parameters:
    level: 5
    paths:
        - app
        - config
        - database
        - resources
        - routes
        - tests
    excludePaths:
        - storage/*
        - vendor/*
    checkMissingIterableValueType: false
    ignoreErrors:
        - '#Access to an undefined property [a-zA-Z0-9\\_]+::\$[a-zA-Z0-9\\_]+#'
        - '#Variable \$this might not be defined#'
        - '#Unknown parameter \$[a-zA-Z0-9\\_]+ in call to method#'